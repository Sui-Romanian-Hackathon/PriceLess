# Example environment configuration for Events Indexer
# Copy this file to .env and configure with your actual values

# Database Configuration
# PostgreSQL connection string for the indexer database
DATABASE_URL=postgresql://username:password@localhost:5432/events-indexer

# Example configurations:
# Local development: postgresql://postgres:postgrespw@localhost:5432/events-indexer
# Production: postgresql://user:pass@host:5432/prod_db

# Optional: TLS certificate for database connection
# DATABASE_TLS_CA_CERT=

# Agent Package ID
# The Sui package ID to monitor for agent events
AGENT_PACKAGE_ID=0xdba8e7ee8416fc05a23bd9938d6b9cecf7e7b4cdb544b4aa2a5f8acfe647a1a0
